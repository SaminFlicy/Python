import random #importing module
playing = True
number = str(random.randint(10,20)) #random in bulid functions



print("I will generate a number from 10 ton 20, and you have to guess the number one digit at a time")


print("The game ends when you get 1 hero!")
#illiterate loop till the condition is true 

while playing:
    guess = input("Give me your best guess! \n")
    if number == guess:
        print("You win the game")
        print("the number was",number)
        break


    else:
     print("Your guess isn't quite right,try again :). \n")

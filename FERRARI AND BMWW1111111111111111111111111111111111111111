# Parent class
class BMW:
 

    def __init__(self,eyes,aggressive):
        
        self.eyes = eyes
        self.aggressive = aggressive
 

    def display(self):
        print("BMW!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!",self.eyes)
        print("You have Awakened your ultra infinite tsukiyomi type whells",self.aggressive)

# Child class
class FERRARI(BMW):



    def __init__(self , name, age, eyes, aggressive):
        
        self.name = name
        self.age = age


